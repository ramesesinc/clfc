"listHandler:item.*" [item.value == 100.00] {
    background: red;
} 
"listHandler:item.value" [item.value == 100.00] {
    background: cyan; color: #fff; 
    text-decoration: underline; 
    text-decoration: strikethrough;
} 
"listHandler:item.(amtpaid|name|value)"[ state == 'read' ]{ 
    background-color: #a5a5a5;
    editable: false; 
} 
