"entity.*" [borrowerContext.mode != 'edit']{
    enabled: false; 
    background-color: rgb(250,250,250);
}
"borrower" [borrowerContext.mode != 'edit']{
    enabled: false; 
    background-color: rgb(250,250,250);
}
"borrower" [borrowerContext.mode == 'edit']{
    enabled: true;
    background-color: #fff;
}
"entity.relation" [borrowerContext.borrower?.type != 'PRINCIPAL'] {
    visible:true;
}
"entity.relation" [borrowerContext.borrower?.type == 'PRINCIPAL'] {
    visible:false;
}
