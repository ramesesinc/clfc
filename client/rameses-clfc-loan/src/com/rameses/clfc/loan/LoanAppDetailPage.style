"entity.*|schedule|clienttype" [caller.mode != 'edit']{
    enabled: false; 
    background-color: rgb(250,250,250);
}
"entity.*|schedule|clienttype" [caller.mode == 'edit']{
    enabled: true;
    background-color: #fff;
}
"entity.marketedby" [caller.mode == 'edit' && clienttype.value != 'MARKETED'] {
    enabled: false; 
    background-color: rgb(250,250,250);
} 
"entity.marketedby" [caller.mode == 'edit' && clienttype.value == 'MARKETED'] {
    enabled: true; 
    background-color: #fff;    
}
"entity.route" [loanapp.state == 'FOR_INSPECTION' && caller.mode != 'edit'] {
    enabled: false; 
    background-color: rgb(250,250,250);
}
"entity.route" [loanapp.state == 'FOR_INSPECTION' && caller.mode == 'edit'] {
    enabled: true;
    background-color: #fff;    
}
"entity.route" [loanapp.state != 'FOR_INSPECTION'] {
    enabled: false; 
    background-color: rgb(250,250,250);
}